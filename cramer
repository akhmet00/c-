#include <iostream>
using namespace std;

int main() {
    int a[3][4];
    int d,d1,d2,d3;
    int x1,x2,x3;
    int D[3][3]; 
    int D1[3][3],D2[3][3],D3[3][3];
    for(int i=0;i<3;i++){
        for(int j=0;j<4;j++){
            cin>>a[i][j];
        }
    }
    for(int i=0;i<3;i++){
        for(int j=0;j<3;j++){
            D[i][j]=a[i][j];
            D1[i][j]=a[i][j];
            D2[i][j]=a[i][j];
            D3[i][j]=a[i][j];
        }
    }
    D1[0][0]=a[0][3]; D2[0][1]=a[0][3]; D3[0][2]=a[0][3];
    D1[1][0]=a[1][3]; D2[1][1]=a[1][3]; D3[1][2]=a[1][3];
    D1[2][0]=a[2][3]; D2[2][1]=a[2][3]; D3[2][2]=a[2][3];
    d=(D[0][0]*D[1][1]*D[2][2]+D[0][1]*D[1][2]*D[2][0]+D[0][2]*D[1][0]*D[2][1])-(D[2][0]*D[1][1]*D[0][2]+D[2][1]*D[1][2]*D[0][0]+D[2][2]*D[1][0]*D[0][1]);
    d1=(D1[0][0]*D1[1][1]*D1[2][2]+D1[0][1]*D1[1][2]*D1[2][0]+D1[0][2]*D1[1][0]*D1[2][1])-(D1[2][0]*D1[1][1]*D1[0][2]+D1[2][1]*D1[1][2]*D1[0][0]+D1[2][2]*D1[1][0]*D1[0][1]);
    d2=(D2[0][0]*D2[1][1]*D2[2][2]+D2[0][1]*D2[1][2]*D2[2][0]+D2[0][2]*D2[1][0]*D2[2][1])-(D2[2][0]*D2[1][1]*D2[0][2]+D2[2][1]*D2[1][2]*D2[0][0]+D2[2][2]*D2[1][0]*D2[0][1]);
    d3=(D3[0][0]*D3[1][1]*D3[2][2]+D3[0][1]*D3[1][2]*D3[2][0]+D3[0][2]*D3[1][0]*D3[2][1])-(D3[2][0]*D3[1][1]*D3[0][2]+D3[2][1]*D3[1][2]*D3[0][0]+D3[2][2]*D3[1][0]*D3[0][1]);
    x1=d1/d;
    x2=d2/d;
    x3=d3/d;
    cout<<"d="<<d<<endl;
    cout<<"x1: "<<x1<<endl;
    cout<<"x2: "<<x2<<endl;
    cout<<"x3: "<<x3<<endl;
